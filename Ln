.global _start
_start:
	ldr r0, _data
	mov r3, #0x7f
	
//----------------
	add r3, r3, r0, lsr #22
	push {r3}
	
	mov r1, #8
	sub r1, r1, r0, lsr #22
	orr r0, #0x00800000
	lsl r0, r1
	adds r1, r0, #0x80000000
	rrx r1, r1
	clz r2, r0
	lsr r1, #6
	subs r0, #0x80000000
	mvnmi r0, r0
	addmi r0, #1
	clz r3, r0
	lsl r0, r3
	add r3, r2
	orrmi r3, #0x80000000
	
	push {r3-r5}
	

_data:
	.word 0x00400000
	
	
